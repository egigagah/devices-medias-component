{
  "name": "react-ide-component",
  "version": "0.0.0",
  "description": "IDE Component with animation for react js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "husky install",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "path": "^0.12.7",
    "prettier": "^2.5.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.49.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "gsap": "^2.0.0 || ^3.9.1",
    "highlight.js": "^11.0.0 || ^11.4.0",
    "html-react-parser": "^1.2.0 || ^1.4.6",
    "react": "^16.8.0 || ^17.0.2",
    "react-dom": "^16.8.0 || ^17.0.2"
  },
  "lint-staged": {
    "*.{tsx}": [
      "eslint . --ext ts --ext tsx --ext js --fix"
    ]
  },
  "keywords": [
    "react",
    "react-component",
    "reactjs",
    "ide",
    "vscode",
    "editor",
    "media",
    "xcode",
    "highlight.js",
    "typewritter",
    "gsap"
  ],
  "files": [
    "dist"
  ],
  "directories": {
    "example": "examples"
  },
  "author": "Egi Gagah Brilliant",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/egigagah/react-ide-component"
  },
  "homepage": "https://github.com/egigagah/react-ide-component",
  "bugs": {
    "url": "https://github.com/egigagah/react-ide-component/issues"
  }
}
